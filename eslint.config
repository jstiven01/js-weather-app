{
  "parserOptions": { 
    "ecmaVersion": 8,
  },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
    'import/extensions': 'off',
    'import/no-unresolved': 'off',
    "import/extensions":[ 1,"ignorePackages"]
  },
  "env": {
   "browser": true,
   "es6": true
  },  
}