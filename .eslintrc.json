{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off"
    }
}